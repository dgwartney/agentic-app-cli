# Kore.ai Agentic App Platform API Configuration
# Copy this file to .env and fill in your actual values

# Required: API key for authentication
KOREAI_API_KEY=your-api-key-here

# Required: Application ID
KOREAI_APP_ID=your-app-id-here

# Optional: Environment name (default: production)
KOREAI_ENV_NAME=production

# Optional: Base URL for API (default: https://agent-platform.kore.ai/api/v2)
KOREAI_BASE_URL=https://agent-platform.kore.ai/api/v2

# Optional: Request timeout in seconds (default: 30)
KOREAI_TIMEOUT=30
